{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"heroes",
      "urls":["https://gateway.marvel.com/**"],
      "cacheConfig":{        
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}

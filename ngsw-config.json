{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"heroes",
      "urls":["https://gateway.marvel.com:443/v1/public/characters/**"],
      "cacheConfig":{
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "12h",
        "timeout": "10s"
      }
    },
    {
      "name":"series",
      "urls":["https://gateway.marvel.com:443/v1/public/series/**"],
      "cacheConfig":{
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "12h",
        "timeout": "10s"
      }
    }


  ]
}
